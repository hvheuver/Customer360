{
	"_version": "1.8.0",
	"sap.app": {
		"id": "com.delaware.hvh.trac2018",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"Northwind.svc": {
				"uri": "/Northwind/V2/Northwind/Northwind.svc",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"ZV_ZVT18_CUSTM_HVH_CDS": {
				"uri": "/sap/opu/odata/sap/ZV_ZVT18_CUSTM_HVH_CDS/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZV_ZVT18_CUSTM_HVH_CDS/metadata.xml"
				}
			},
			"ZV_ZVT18_ORDERS_HVH_CDS": {
				"uri": "/sap/opu/odata/sap/ZV_ZVT18_ORDERS_HVH_CDS/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZV_ZVT18_ORDERS_HVH_CDS/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.delaware.hvh.trac2018.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.delaware.hvh.trac2018.i18n.i18n"
				}
			},
			"appView": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false
			},
			"": {
				"uri": "/sap/opu/odata/sap/ZV_ZVT18_CUSTM_HVH_CDS/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "ZV_ZVT18_CUSTM_HVH_CDS",
				"preload": true
			},
			"customersModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false
			},
			"ordersModel": {
				"uri": "/sap/opu/odata/sap/ZV_ZVT18_ORDERS_HVH_CDS/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "ZV_ZVT18_ORDERS_HVH_CDS",
				"preload": true
			},
			"orderViewModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false
			},
			"customerModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.delaware.hvh.trac2018.view",
				"controlId": "idAppControl"
			},
			"routes": [{
				"name": "Master",
				"pattern": "",
				"target": "Master"
			}, {
				"name": "Detail",
				"pattern": "customer/{customerNumber}",
				"target": ["Master", "Detail"]
			}],
			"targets": {
				"Master": {
					"viewType": "XML",
					"viewName": "Master",
					"routerClass": "sap.f.routing.Router",
					"async": true,
					"viewPath": "com.delaware.hvh.trac2018.view",
					"controlId": "idAppControl",
					"controlAggregation": "beginColumnPages"
				},
				"Detail": {
					"viewType": "XML",
					"viewName": "Detail",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	}
}